{
  "name": "espresso",
  "version": "0.0.1",
  "description": "The Espresso microservice wrapper",
  "main": "service.js",
  "scripts": {
    "test": "mocha --require babel-polyfill --compilers js:babel-register  ./**/*.spec.js",
    "clean": "rm  -rf dist",
    "start": "node bin/dev",
    "build": "npm run clean && mkdir dist && babel ./lib/ -s -d dist",
    "prod": "npm run build && npm test && node bin/prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadimri/espresso.git"
  },
  "author": "VadimRi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vadimri/espresso/issues"
  },
  "homepage": "https://github.com/vadimri/espresso#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-node8-es6": "^12.0.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "request": "^2.81.0"
  },
  "dependencies": {
    "express": "^4.15.4"
  },
  "engines": {
    "node": ">=8.4.0"
  }
}
