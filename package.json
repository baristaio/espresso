{
  "name": "espresso",
  "version": "0.0.1",
  "description": "The Espresso microservice wrapper",
  "main": "service.js",
  "scripts": {
    "test": "mocha --require babel-polyfill --compilers js:babel-register  ./**/*.spec.js",
    "clean": "rm  -rf dist",
    "start": "node bin/dev",
    "build": "npm run clean && mkdir dist && babel ./lib/ -s -d dist",
    "postinstall": "npm run build",
    "prod": "npm run build && npm run test && node ./bin/prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadimri/espresso.git"
  },
  "author": "vadimri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vadimri/espresso/issues"
  },
  "homepage": "https://github.com/vadimri/espresso#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-node8-es6": "^12.0.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "request": "^2.83.0"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "cors": "^2.8.4",
    "deep-extend": "^0.5.1",
    "express": "^4.16.2",
    "express-bunyan-logger": "^1.3.2",
    "yarn": "^1.6.0"
  },
  "engines": {
    "node": ">=8.11.1"
  }
}
